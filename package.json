{
  "name": "test-driven-development-with-golang",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4201",
    "app:start:aot": "ng serve --aot",
    "dev:run": "run-p app:start:aot server:run:watch",
    "server:build": "run-s server:go:path server:compile server:run",
    "server:compile": "go build -o dist/main server/src/main.go",
    "server:go:path": "export GOPATH=/Users/rodrigosilveira/projects/TestDrivenDevelopmentWithGolang",
    "server:run": "./dist/main",
    "server:test": "yarn server:go:path && go test -v -count=1 ./server/src",
    "server:run:watch": "nodemon --delay 2000ms --signal SIGINT --watch server --ext go --ignore *.test.go --exec 'yarn server:build'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.4",
    "@angular/cdk": "~8.1.4",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/forms": "~8.2.4",
    "@angular/material": "^8.1.0",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/router": "~8.2.4",
    "ngx-markdown": "~8.1.0",
    "rxjs": "~6.5.2",
    "tslib": "~1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.2",
    "@angular/cli": "~8.3.2",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@types/chai": "^4.2.0",
    "@types/cucumber": "^4.0.7",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "chai": "^4.2.0",
    "codelyzer": "~5.0.0",
    "cucumber": "^5.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "protractor-cucumber-framework": "^6.1.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}
